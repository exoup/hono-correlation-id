{
    "compilerOptions": {
        "rootDir": "./src",
        "outDir": "./dist",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "types": [
            "jest",
            "node",
            "@cloudflare/workers-types"
        ]
    },
    "include": [
        "./src/**/*.ts"
    ]
}
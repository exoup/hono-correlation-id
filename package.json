{
  "name": "hono-correlation-id",
  "version": "1.0.3",
  "description": "Hono middleware to set correlation id across requests.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest --run",
    "build": "tsup ./src/index.ts --format esm,cjs --dts",
    "publint": "publint",
    "release": "pnpm build && pnpm test && pnpm publint && pnpm publish"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exoup/hono-correlation-id.git"
  },
  "homepage": "https://github.com/exoup/hono-correlation-id#readme",
  "keywords": [
    "hono",
    "correlation",
    "request",
    "id",
    "middleware"
  ],
  "author": "JosephY (https://github.com/exoup)",
  "license": "MIT",
  "peerDependencies": {
    "hono": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240529.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.7",
    "hono": "^4.4.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "skipNodeModulesBundle": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "target": "es2020",
    "outDir": "dist",
    "entry": [
      "src/**/*.ts"
    ]
  }
}